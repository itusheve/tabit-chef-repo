{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "dashboard"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "manifest.json"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "serviceWorker": false,
      "styles": [
        "../node_modules/bootstrap/dist/css/bootstrap-reboot.min.css",
        "../node_modules/bootstrap/dist/css/bootstrap-grid.min.css",
        "../node_modules/devextreme/dist/css/dx.common.css",
        "../node_modules/devextreme/dist/css/dx.light.css",
        "styles.scss"
      ],
      "scripts": [
        "./js/lib/OrderView_v1/OrderView.js",
        "./js/lib/OrderView_v1/Translate.js",
        "./js/lib/OrderView_v1/Utils.js",
        "./js/vendor/rpbouman-xmla4j/xmla-min.js",
        "./js/xmla4j-wrapper.class.js",
        "../node_modules/moment-timezone/builds/moment-timezone-with-data-2012-2022.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "dev-il": "environments/environment.dev-il.ts",
        "dev-us": "environments/environment.dev-us.ts",
        "prod-il": "environments/environment.prod-il.ts",
        "prod-us": "environments/environment.prod-us.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}