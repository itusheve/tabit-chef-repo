{
  "name": "dashboard",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "ng build --target=production --env=prod-il --output-hashing none --i18nFile=src/locale/messages.he.xlf --i18nFormat=xlf --locale=he",
    "ng": "ng",
    "start": "node server.js",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev-il": "ng serve --env=dev-il --host 0.0.0.0 --i18nFile=src/locale/messages.he.xlf --i18nFormat=xlf --locale he",
    "dev-us": "ng serve --env=dev-us --host 0.0.0.0",
    "dev-il-aot": "ng serve --aot --env=dev-il --host 0.0.0.0 --i18nFile=src/locale/messages.he.xlf --i18nFormat=xlf --locale he",
    "dev-us-aot": "ng serve --aot --env=dev-us --host 0.0.0.0",
    "build-il": "ng build --aot --env=prod-il --target=production --i18nFile=src/locale/messages.he.xlf --i18nFormat=xlf --locale=he",
    "build-us": "ng build --aot --env=prod-us --target=production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.7",
    "@angular/cdk": "^6.0.0-beta.2",
    "@angular/cli": "^1.7.2",
